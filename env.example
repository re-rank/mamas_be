# Environment
ENVIRONMENT=development

# Qdrant Settings
QDRANT_URL=https://3d64fa5a-33ce-43f3-bf39-9ad85f5ef0ee.us-west-1-0.aws.cloud.qdrant.io:6333
QDRANT_API_KEY=your_qdrant_api_key_here

# OpenAI Settings
OPENAI_API_KEY=your_openai_api_key_here

# VoyageAI Settings (Optional - for better embeddings)
VOYAGE_API_KEY=your_voyage_api_key_here
USE_VOYAGE_EMBEDDING=true

# LLM Settings
LLM_MODEL=gpt-4o-mini
LLM_TEMPERATURE=0.7
LLM_MAX_TOKENS=2000

# Search Settings
DEFAULT_SEARCH_K=5
MAX_SEARCH_K=20
SEARCH_SCORE_THRESHOLD=0.3

# Server Settings
HOST=0.0.0.0
PORT=8000

# CORS Settings
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# Cache Settings
ENABLE_SEARCH_CACHE=true
CACHE_TTL_SECONDS=300
CACHE_MAX_SIZE=100

# Collection Name (Qdrant)
COLLECTION_NAME=labor_consultant_docs

